                            app:layout_constraintTop_toBottomOf="@+id/doc2qual" />

                        <TextView
                            android:id="@+id/doc2exp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="12dp"
                            android:textColor="#000000"
                            android:text="Experience : 4 years"
                            app:layout_constraintStart_toEndOf="@+id/phydoc2"
                            app:layout_constraintTop_toBottomOf="@+id/doc2fees" />

                        <Button
                            android:id="@+id/doc2no"
                            android:layout_width="121dp"
                            android:layout_height="38dp"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="40dp"
                            android:background="@color/trans"
                            android:text="Book an appointment"
                            android:textColor="#A50B5E"
                            app:layout_constraintStart_toEndOf="@+id/phydoc2"
                            app:layout_constraintTop_toBottomOf="@+id/doc2exp" />

                        <TextView
                            android:id="@+id/doc2area"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="12dp"
                            android:textColor="#000000"
                            android:text="Area : Khar (West)"
                            app:layout_constraintStart_toEndOf="@+id/phydoc2"
                            app:layout_constraintTop_toBottomOf="@+id/doc2exp" />

                        <Button
                            android:id="@+id/doc2loc"
                            android:layout_width="wrap_content"
                            android:layout_height="41dp"
                            android:layout_marginStart="36dp"
                            android:layout_marginLeft="36dp"
                            android:layout_marginTop="40dp"
                            android:background="@color/trans"
                            android:text="Locate on maps"
                            android:textColor="#A50B5E"
                            android:onClick="browser2"
                            app:layout_constraintStart_toEndOf="@+id/doc2no"
                            app:layout_constraintTop_toBottomOf="@+id/doc2exp" />

                    </android.support.constraint.ConstraintLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/phycard3"
                    android:layout_width="match_parent"
                    android:layout_height="235dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    app:cardBackgroundColor="#FEFEFE"
                    app:cardCornerRadius="18dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/phycard2"
                    app:cardElevation="4dp">

                    <android.support.constraint.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/phydoc3"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_marginStart="8dp"
                            android:layout_marginLeft="8dp"
                            android:layout_marginTop="28dp"
                            android:src="@drawable/female10"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/doc3name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="28dp"
                            android:textColor="#000000"
                            android:text="Name : Dr.Sangeeta Agrawal"
                            app:layout_constraintStart_toEndOf="@+id/phydoc3"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/doc3qual"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="12dp"
                            android:textColor="#000000"
                            android:text="Qualification : MS(Obstetrics and Gynaecology)"
                            app:layout_constraintStart_toEndOf="@+id/phydoc3"
                            app:layout_constraintTop_toBottomOf="@+id/doc3name" />

                        <TextView
                            android:id="@+id/doc3fees"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="12dp"
                            android:text="Visiting Fee : Rs.700/-"
                            android:textColor="#000000"
                            app:layout_constraintStart_toEndOf="@+id/phydoc3"
                            app:layout_constraintTop_toBottomOf="@+id/doc3qual" />

                        <TextView
                            android:id="@+id/doc3exp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="12dp"
                            android:textColor="#000000"
                            android:text="Experience : 4 years"
                            app:layout_constraintStart_toEndOf="@+id/phydoc3"
                            app:layout_constraintTop_toBottomOf="@+id/doc3fees" />

                        <Button
                            android:id="@+id/doc3no"
                            android:layout_width="121dp"
                            android:layout_height="38dp"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="40dp"
                            android:background="@color/trans"
                            android:textColor="#A50B5E"
                            android:text="Book an appointment"
                            app:layout_constraintStart_toEndOf="@+id/phydoc3"
                            app:layout_constraintTop_toBottomOf="@+id/doc3exp" />

                        <TextView
                            android:id="@+id/doc3area"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40dp"
                            android:layout_marginLeft="40dp"
                            android:layout_marginTop="12dp"
                            android:textColor="#000000"
                            android:text="Area : Prabhadevi"
                            app:layout_constraintStart_toEndOf="@+id/phydoc3"
                            app:layout_constraintTop_toBottomOf="@+id/doc3exp" />

                        <Button
                            android:id="@+id/doc3loc"
                            android:layout_width="wrap_content"
                            android:layout_height="41dp"
                            android:layout_marginStart="36dp"
                            android:layout_marginLeft="36dp"
                            android:layout_marginTop="40dp"
                            android:background="@color/trans"
                            android:textColor="#A50B5E"
                            android:text="Locate on maps"
                            android:onClick="browser3"
                            app:layout_constraintStart_toEndOf="@+id/doc3no"
                            app:layout_constraintTop_toBottomOf="@+id/doc3exp" />

                    </android.support.constraint.ConstraintLayout>
                </android.support.v7.widget.CardView>

            </android.support.constraint.ConstraintLayout>
        </android.support.v4.widget.NestedScrollView>
    </android.support.constraint.ConstraintLayout>
</android.support.design.widget.CoordinatorLayout>